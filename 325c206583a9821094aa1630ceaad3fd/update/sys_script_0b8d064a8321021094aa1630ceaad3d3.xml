        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Add your code here
	var study_schedule_id = current.study_schedule_id;//学习计划编号
	var student_id = current.student_id;//学员编号
	var gr = new GlideRecord('x_1093221_netwisdo_x_student_study_schedule_condition');
	gr.addQuery('study_schedule_id',study_schedule_id);
	gr.addQuery('student_id',student_id);
	gr.addActiveQuery();
	gr.query();

	if(gr.next() && gr.getUniqueValue() != current.getUniqueValue()){
		gs.addErrorMessage('选择的内容已重复！'
							+ '\n' + 
							'学生编号：'+ gr.getDisplayValue('student_id')  
							+ '\n' + 
							'学习计划编号：'+gr.getDisplayValue('study_schedule_id'));
	}else if(gr.getUniqueValue() == current.getUniqueValue()){
		gs.addInfoMessage('数据已更新成功');
	}else{
		gs.addInfoMessage('数据已添加成功');
	}
	
})(current, previous);]]></script>
